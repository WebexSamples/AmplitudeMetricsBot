{
  "type": "metricsBotConfig",
  "body": {
    "measures": "uniques",
    "chart_type": "stacked bar",
    "interval": "daily",
    "interval_range": "10d",
    "dates_between": "YYYYMMDD - YYYYMMDD",
    "repeat": "f",
    "repeat_interval": "daily",
    "alerts": "t",
    "events": [
      {
        "event_type": "client_ecm_add_account_funnel"
      },
      {
        "event_type": "client_ecm_file_share_funnel"
      }
    ],
    "thresholds": [
      "A + B >= 60",
      "A - B < 70"
    ],
    "formulas": [
      "AVG(A) - AVG(B)"
    ]
  }
}
